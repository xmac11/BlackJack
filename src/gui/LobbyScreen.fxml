		<Label layoutX="14.0" layoutY="300.0" text="Chat" />
		<Button fx:id="joinButton" layoutX="663.0" layoutY="14.0" mnemonicParsing="false" onAction="#joinQueue" prefHeight="64.0" prefWidth="121.0" text="Join Game Queue" />
